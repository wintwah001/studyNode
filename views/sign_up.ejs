<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
<% include  partial/head.ejs %>

  <div class="container">
    <h1>Signup with get Method</h1>
    <form class="" action="/users/detail" method="get">
      <div class="form-group">
        <label for="username">Name:</label>
        <input type="text" class="form-control" id="username" name="name" value="">
      </div>
      <div class="form-group">
        <label for="useremail">Email:</label>
      <input type="email" class="form-control" id="useremail" name="email" value="">
    </div>
      <div class="form-group">
        <label for="userpwd">Password:</label>
        <input type="password" class="form-control" id="userpwd" name="pwd" value="">
      </div>
          <input type="submit" class="btn btn-warning" name="submit" value="Save">

    </form>
  </div>
</body>

</html>
